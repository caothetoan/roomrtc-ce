{
  "extensionsLocationCache": true,
  "logger": {
    "console": {
      "transport": "console",
      "level": "info"
    }
  },
  "extensions": null,
  "db": {
    "connection": "memory",
    "dataSource": [{
      "name": "memory",
      "driver": "memory",
      "active": true
    }]
  },
  "express": {
    "static": "./public",
    "httpPort": 3000,
    "poweredBy": "RoomRTC Community Edition",
    "views": {
      "ext": "ejs",
      "engine": "ejs",
      "renderer": "renderFile",
      "path": "content/views"
    }
  }
}
