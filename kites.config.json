{
  "tls": {
    "port": 3443,
    "cert": "docs/cert/server.crt",
    "key": "docs/cert/server.key"
  },
  "extensionsLocationCache": true,
  "logger": {
    "console": {
      "transport": "console",
      "level": "info"
    }
  },
  "extensions": null,
  "db": {
    "connection": "mongodb-memory",
    "dbName": "roomrtc",
    "dataSource": [{
        "name": "memory",
        "driver": "memory",
        "active": true
      },
      {
        "name": "mongodb-memory",
        "driver": "mongodb-memory-server",
        "port": "27071",
        "version": "4.0.3",
        "active": true
      }
    ]
  },
  "express": {
    "static": "./public",
    "httpPort": 3000,
    "poweredBy": "RoomRTC Community Edition",
    "views": {
      "ext": "ejs",
      "engine": "ejs",
      "renderer": "renderFile",
      "path": "content/views"
    }
  },
  "mediasoup": {
    "numWorkers": 0,
    "worker": {
      "rtcMinPort": 10000,
      "rtcMaxPort": 10100,
      "logLevel": "warn",
      "logTags": [
        "info",
        "ice",
        "dtls",
        "rtp",
        "srtp",
        "rtcp"
      ]
    },
    "routerOptions": {
      "mediaCodecs": [{
          "kind": "audio",
          "mimeType": "audio/opus",
          "clockRate": 48000,
          "channels": 2
        },
        {
          "kind": "video",
          "mimeType": "video/VP8",
          "clockRate": 90000,
          "parameters": {
            "x-google-start-bitrate": 1000
          }
        }
      ]
    },
    "webRtcTransportOptions": {
      "listenIps": [{
        "ip": "127.0.0.1",
        "announcedIp": null
      }],
      "maxIncomingBitrate": 1500000,
      "initialAvailableOutgoingBitrate": 1000000
    }
  }
}
